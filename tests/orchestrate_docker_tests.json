{
  "test_suite": "OrchestrateOS Docker Installation Validation",
  "ngrok_url": "https://supposedly-faithful-termite.ngrok-free.app",
  "tests": [
    {
      "name": "check_credits",
      "description": "Verify Docker container is running and accessible",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "check_credits",
        "action": "check_credits",
        "params": {}
      },
      "expected_status": "success",
      "critical": true
    },
    {
      "name": "list_available_tools",
      "description": "Get all available tools from system_settings",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "system_settings",
        "action": "list_tools",
        "params": {}
      },
      "expected_status": "success",
      "critical": true
    },
    {
      "name": "json_manager_create",
      "description": "Test json_manager create operation",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "json_manager",
        "action": "create_json_file",
        "params": {
          "filename": "test_data.json"
        }
      },
      "expected_status": "success",
      "critical": false
    },
    {
      "name": "json_manager_add_entry",
      "description": "Test json_manager add entry operation",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "json_manager",
        "action": "add_json_entry",
        "params": {
          "filename": "test_data.json",
          "entry_key": "test_entry",
          "entry_data": {"test": "data", "timestamp": "2025-11-08"}
        }
      },
      "expected_status": "success",
      "critical": false
    },
    {
      "name": "json_manager_read",
      "description": "Test json_manager read operation",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "json_manager",
        "action": "read_json_file",
        "params": {
          "filename": "test_data.json"
        }
      },
      "expected_status": "success",
      "critical": false
    },
    {
      "name": "file_ops_read",
      "description": "Test file_ops_tool read operation on mike_maples.txt",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "file_ops_tool",
        "action": "read_file",
        "params": {
          "filename_fragment": "mike_maples.txt"
        }
      },
      "expected_status": "success",
      "critical": false
    },
    {
      "name": "unlock_outline_editor",
      "description": "Unlock outline_editor tool",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "unlock_tool",
        "action": "unlock_tool",
        "params": {
          "tool_name": "outline_editor"
        }
      },
      "expected_status": "success",
      "critical": false
    },
    {
      "name": "set_outline_credentials",
      "description": "Set Outline API credentials",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "system_settings",
        "action": "set_credential",
        "params": {
          "script_path": "tools/outline_editor.py",
          "value": "ol_api_a8LDBwTsv0NSHpslsedA8XkmaTggYcgsY8AkK9"
        }
      },
      "expected_status": "success",
      "critical": true
    },
    {
      "name": "create_dogs_collection",
      "description": "Create Dogs collection",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "outline_editor",
        "action": "create_collection",
        "params": {
          "name": "Dogs",
          "description": "Test collection for German Shepherds",
          "permission": "read_write",
          "icon": "dog",
          "color": "#FF0000",
          "sharing": true
        }
      },
      "expected_status": "success",
      "expected_contains": "Dogs",
      "critical": false
    },
    {
      "name": "create_german_shepherds_doc",
      "description": "Create German Shepherds document in Dogs collection",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "outline_editor",
        "action": "create_doc",
        "params": {
          "title": "German Shepherds",
          "text": "# German Shepherds\n\nGerman Shepherds are intelligent, loyal, and versatile working dogs.\n\n## Characteristics\n- Highly trainable\n- Strong and athletic\n- Excellent guard dogs\n\n#Dogs",
          "publish": true
        }
      },
      "expected_status": "success",
      "expected_contains": "German Shepherds",
      "critical": false
    }
  ],
  "cleanup": [
    {
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "outline_editor",
        "action": "delete_collection",
        "params": {
          "name": "Dogs"
        }
      }
    }
  ]
}
