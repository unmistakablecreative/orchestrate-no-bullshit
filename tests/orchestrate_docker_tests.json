{
  "test_suite": "OrchestrateOS Docker Installation Validation",
  "ngrok_url": "https://supposedly-faithful-termite.ngrok-free.app",
  "tests": [
    {
      "name": "check_credits",
      "description": "Verify Docker container is running and accessible",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "check_credits",
        "action": "check_credits",
        "params": {}
      },
      "expected_status": "success",
      "critical": true
    },
    {
      "name": "list_available_tools",
      "description": "Get all available tools from system_settings",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "system_settings",
        "action": "list_tools",
        "params": {}
      },
      "expected_status": "success",
      "critical": true
    },
    {
      "name": "json_manager_create",
      "description": "Test json_manager create operation",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "json_manager",
        "action": "create_json_file",
        "params": {
          "filename": "test_data.json"
        }
      },
      "expected_status": "success",
      "critical": false
    },
    {
      "name": "json_manager_add_entry",
      "description": "Test json_manager add entry operation",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "json_manager",
        "action": "add_json_entry",
        "params": {
          "filename": "test_data.json",
          "entry_key": "test_entry",
          "entry_data": {"test": "data", "timestamp": "2025-11-08"}
        }
      },
      "expected_status": "success",
      "critical": false
    },
    {
      "name": "json_manager_read",
      "description": "Test json_manager read operation",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "json_manager",
        "action": "read_json_file",
        "params": {
          "filename": "test_data.json"
        }
      },
      "expected_status": "success",
      "critical": false
    },
    {
      "name": "unlock_outline_editor",
      "description": "Unlock outline_editor tool (pre-installed, just marks unlocked)",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "unlock_tool",
        "action": "unlock_tool",
        "params": {
          "tool_name": "outline_editor"
        }
      },
      "expected_status": "already_unlocked",
      "critical": false
    },
    {
      "name": "set_outline_credentials",
      "description": "Set Outline API credentials",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "system_settings",
        "action": "set_credential",
        "params": {
          "script_path": "tools/outline_editor.py",
          "value": "ol_api_a8LDBwTsv0NSHpslsedA8XkmaTggYcgsY8AkK9"
        }
      },
      "expected_status": "success",
      "critical": true
    },
    {
      "name": "create_dogs_collection",
      "description": "Create a collection called Dogs",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "outline_editor",
        "action": "create_collection",
        "params": {
          "name": "Dogs",
          "description": "Collection for dog-related docs",
          "permission": "read_write",
          "icon": "üêï",
          "color": "#FF9900",
          "sharing": true
        }
      },
      "expected_status": 200,
      "critical": false
    },
    {
      "name": "create_golden_retriever_doc",
      "description": "Create a doc called Golden Retriever in Dogs collection",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "outline_editor",
        "action": "create_doc",
        "params": {
          "title": "Golden Retriever",
          "content": "Golden Retrievers are friendly, intelligent, and devoted dogs.",
          "collectionId": "Dogs"
        }
      },
      "expected_status": 200,
      "critical": false
    },
    {
      "name": "unlock_claude_assistant",
      "description": "Unlock claude_assistant (triggers OAuth, auto-waits for auth completion)",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "unlock_tool",
        "action": "unlock_tool",
        "params": {
          "tool_name": "claude_assistant"
        }
      },
      "expected_status": "success",
      "critical": true
    },
    {
      "name": "verify_claude_actions_registered",
      "description": "Verify claude_assistant actions are now available in system",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "system_settings",
        "action": "list_supported_actions",
        "params": {}
      },
      "expected_status": "success",
      "expected_contains": "claude_assistant",
      "critical": true
    },
    {
      "name": "test_autonomous_execution",
      "description": "Test autonomous task execution via Claude assistant",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "claude_assistant",
        "action": "assign_task",
        "params": {
          "task_id": "docker_install_test_exec",
          "description": "Create a text file at data/autonomous_execution_confirmed.txt with content: 'Docker installation test passed - autonomous execution verified at [current timestamp]'",
          "priority": "high"
        }
      },
      "expected_status": "success",
      "critical": true
    },
    {
      "name": "verify_autonomous_execution",
      "description": "Check that Claude task was created and has status",
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "claude_assistant",
        "action": "check_task_status",
        "params": {
          "task_id": "docker_install_test_exec"
        }
      },
      "expected_status": "success",
      "expected_contains": "task_status",
      "critical": true
    }
  ],
  "cleanup": [
    {
      "endpoint": "/execute_task",
      "payload": {
        "tool_name": "claude_assistant",
        "action": "cancel_task",
        "params": {
          "task_id": "docker_install_test_exec"
        }
      }
    }
  ]
}